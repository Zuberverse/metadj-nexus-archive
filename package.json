{
  "name": "metadj-nexus",
  "version": "0.9.46",
  "description": "MetaDJ Nexus - The primary creative hub for MetaDJ featuring Music, Cinema, Wisdom, and MetaDJai experiences",
  "keywords": [
    "music",
    "cinema",
    "virtualizer",
    "wisdom",
    "cosmos",
    "metadjai",
    "metadj-nexus",
    "metadj",
    "next.js",
    "web-audio-api",
    "ai-companion"
  ],
  "author": "Zuberant",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https -H 0.0.0.0 -p ${PORT:-8100}",
    "dev:webpack": "next dev --webpack --experimental-https -H 0.0.0.0 -p ${PORT:-8100}",
    "dev:http": "next dev -H ${HOST:-0.0.0.0} -p ${PORT:-8100}",
    "dev:replit": "next dev -H 0.0.0.0 -p 5000",
    "prebuild": "npm run lint && npm run type-check && npm test",
    "prebuild:ci": "npm run test:ci",
    "build": "cross-env NODE_ENV=production next build",
    "build:ci": "cross-env NODE_ENV=production next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8100}",
    "start:replit": "next start -H 0.0.0.0 -p 5000",
    "analyze": "ANALYZE=true npm run build",
    "lint": "eslint --max-warnings=0 .",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix .",
    "pretest": "node scripts/validate-music.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ci": "npm run lint && npm run type-check && npm test && npm run test:e2e",
    "test:api": "vitest run tests/api --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "clean": "rm -rf .next .turbo node_modules/.cache coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/devtools": "^0.0.4",
    "@ai-sdk/google": "^3.0.2",
    "@ai-sdk/mcp": "^1.0.6",
    "@ai-sdk/openai": "^3.0.2",
    "@ai-sdk/xai": "^3.0.3",
    "@aws-sdk/client-s3": "^3.962.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@replit/object-storage": "^1.0.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "ai": "^6.0.6",
    "clsx": "^2.1.1",
    "driver.js": "^1.4.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "next": "^16.1.1",
    "openai": "^6.10.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "turndown": "^7.2.2",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^16.0.8",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.10",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
